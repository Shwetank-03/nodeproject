<button><a href="/">Logout</a></button>
<div id="update-record-container" class="update-record-container">
    <p>Teacher</p>
    <form action="/teacher/edit" class="form-group" method="post">
        <label for="rollno">Roll Number</label>
        <input id="rollno" type="number" name="rollno" value="<%= data.rollno %>"/>
        <label for="name">Name</label>
        <input id="name" name="name" type="text" value="<%= data.name %>"/>
        <label for="dob">Date of birth</label>
        <input id="dob" type="date" name="dob" value="<%= data.dob %>"/>
        <label for="score">Score</label>
        <input id="score" type="number" name="score" value="<%= data.score %>"/>
        <input type="submit" value="Update" name="submit" />
    </form>
</div>